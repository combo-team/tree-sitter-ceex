{
  "grammars": [
    {
      "name": "ceex",
      "camelcase": "Ceex",
      "scope": "source.ceex",
      "path": ".",
      "file-types": ["ceex"],
      "highlights": "queries/highlights.scm",
      "injection-regex": "^(ceex)$"
    }
  ],
  "metadata": {
    "version": "0.8.1",
    "license": "MIT",
    "description": "Tree-sitter grammar for CEEx templates",
    "authors": [
      {
        "name": "Clay"
      }
    ],
    "links": {
      "repository": "https://github.com/combo-team/tree-sitter-ceex"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true
  }
}
